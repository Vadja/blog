<% if user_signed_in? %>
    <span>Welcome, <%= current_user.email %></span><br>
    <%= link_to "Watch blogs", controller: "posts" %><br>
    <%= link_to 'Exit', destroy_user_session_path, :method => :delete %>
<% else %>
    <%= link_to "My Blog", controller: "posts" %><br>
    <%= link_to 'Enter', new_user_session_path %> or <%= link_to 'Register', new_user_registration_path %>
<% end %>